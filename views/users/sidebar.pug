aside.main-sidebar
    // sidebar: style can be found in sidebar.less
    section.sidebar
        // Sidebar user panel
        .user-panel
            .pull-left.image
                img.img-circle(src='/adminlte/dist/img/user2-160x160.jpg' alt='User Image')
            .pull-left.info
                p #{session_store.username}
                a(href='#')
                    i.fa.fa-circle.text-success
                    |  Online
        ul.sidebar-menu(data-widget='tree')
            li.header MAIN NAVIGATION
                li.active.treeview
                    a(href='#')
                        i.fa.fa-dashboard
                        span Dashboard
                if(session_store.admin == true)
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Profil
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/inputprofil')
                                    i.fa.fa-circle-o
                                    |  Input Data Profil
                            li
                                a(href='/dataprofil')
                                    i.fa.fa-circle-o
                                    |  Lihat Data Profil
                else
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Profil
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
                        ul.treeview-menu
                            li
                                a(href='/dataprofilmember')
                                    i.fa.fa-circle-o
                                    |  Lihat Data Profil
                if(session_store.admin == true)
                    li.treeview
                        a(href='#')
                            i.fa.fa-laptop
                            span Data Jadwal
                            span.pull-right-container
                                i.fa.fa-angle-left.pull-right
